{
  "author": "skellertor",
  "name": "react.boilerplate",
  "version": "1.0.1",
  "description": "A react app boilerplate with state management, testing libs, linting, and npm scripts already included",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.5.1",
    "babel-preset-minify": "^0.2.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^4.1.2",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.9",
    "enzyme": "^3.3.0",
    "eslint": "^4.16.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.6.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.0.0",
    "node-sass": "^4.7.2",
    "redux-devtools": "^3.4.1",
    "redux-devtools-chart-monitor": "^1.7.0",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-log-monitor": "^1.4.0",
    "redux-slider-monitor": "^2.0.0-1",
    "sass-lint": "^1.12.1",
    "sass-loader": "^6.0.6",
    "sinon": "^2.3.1",
    "sinon-chai": "^2.10.0",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack-dev-server": "^2.11.0"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "babel-preset-es2015": "^6.24.1",
    "history": "^4.7.2",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "webpack": "^3.10.0"
  },
  "scripts": {
    "local:build": "webpack --env.NODE_ENV=local --config webpack.config.js",
    "local:server": "npm run local:build && webpack-dev-server --env.NODE_ENV=local --content-base dist/ --open",
    "dev:build": "webpack --env.NODE_ENV=dev --config webpack.config.js",
    "dev:server": "npm run dev:build && webpack-dev-server --env.NODE_ENV=dev --content-base dist/ --open",
    "stage:build": "webpack --env.NODE_ENV=stage --config webpack.config.js",
    "stage:server": "npm run stage:build && webpack-dev-server --env.NODE_ENV=stage --content-base dist/ --open",
    "prod:build": "BABEL_ENV=prod webpack --env.NODE_ENV=prod --config webpack.config.js",
    "prod:server": "npm run prod:build && webpack-dev-server --env.NODE_ENV=prod --content-base dist/ --open",
    "test": "NODE_ENV=development mocha --compilers js:babel-core/register --require ignore-styles --require ./src/config/test-setup.js --recursive ./src/**/*.test.js",
    "lint": "eslint 'src/**/*.js'",
    "lint:fix": "eslint --fix 'src/**/*.js'",
    "lint:sass": "sass-lint -c .sass-lint.yml 'src/**/*.scss' -v -q"
  }
}
